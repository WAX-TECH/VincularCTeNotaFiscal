                <ui:InvokeCode ContinueOnError="{x:Null}" Code="' Carregue o XML a partir da variável que contém o caminho do arquivo XML&#xA;Dim xml As XDocument = XDocument.Load(xmlFile)&#xA;&#xA;' Verifique se o XML usa namespaces&#xA;Dim ns As XNamespace = xml.Root.GetDefaultNamespace()&#xA;&#xA;' Inicialize a DataTable&#xA;Dim dtCTe As New DataTable&#xA;dtCTe.Columns.Add(&quot;Toma&quot;, GetType(String))&#xA;dtCTe.Columns.Add(&quot;nCT&quot;, GetType(String))&#xA;dtCTe.Columns.Add(&quot;Remetente&quot;, GetType(String))&#xA;dtCTe.Columns.Add(&quot;Recebedor&quot;, GetType(String))&#xA;dtCTe.Columns.Add(&quot;MunRecebedor&quot;, GetType(String))&#xA;&#xA;' Iterar sobre os elementos do XML&#xA;For Each item As XElement In xml.Descendants(ns + &quot;infCte&quot;)&#xA;    ' Inicializar variável para 'toma'&#xA;    Dim toma As String = String.Empty&#xA;&#xA;    ' Tentar capturar 'toma3', e se falhar, capturar 'toma4'&#xA;    Try&#xA;        toma = item.Element(ns + &quot;ide&quot;).Element(ns + &quot;toma3&quot;).Element(ns + &quot;toma&quot;).Value&#xA;    Catch ex As Exception&#xA;&#x9;&#x9;toma = item.Element(ns + &quot;ide&quot;).Element(ns + &quot;toma4&quot;).Element(ns + &quot;toma&quot;).Value&#xA;    End Try&#xA;&#xA;    ' Captura os outros dados normalmente&#xA;    Dim nCT As String = item.Element(ns + &quot;ide&quot;).Element(ns + &quot;nCT&quot;).Value&#xA;    Dim remetente As String = item.Element(ns + &quot;emit&quot;).Element(ns + &quot;xNome&quot;).Value ' Nome do remetente&#xA;    Dim recebedor As String = item.Element(ns + &quot;dest&quot;).Element(ns + &quot;xNome&quot;).Value ' Nome do recebedor&#xA;    Dim munRecebedor As String = item.Element(ns + &quot;dest&quot;).Element(ns + &quot;enderDest&quot;).Element(ns + &quot;xMun&quot;).Value ' Município do recebedor&#xA;&#xA;    ' Adicione os valores na DataTable&#xA;    dtCTe.Rows.Add(toma, nCT, remetente, recebedor, munRecebedor)&#xA;Next&#xA;&#xA;' Atribuir a DataTable 'dtCTe' ao argumento de sa